{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "eact/react-in-jsx-scope": 0,
    "react/jsx-uses-react": 0,
    "no-use-before-define": 0,
    "react/function-component-definition": [
      2,
      { "namedComponents": "function-declaration" }
    ]
  }
}
